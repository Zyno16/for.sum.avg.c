#include <stdio.h>
#include <stdlib.h>

int main()
{
    float t[10];
    int i;
    float s,m,p;
    for(i=0;i<10;i++){
        printf("t[%d]=",i+1);
        scanf("%f",&t[i]);
    }s=0;
    p=1;
    for(i=0;i<10;i++){
        s=s+t[i];
    p=p*t[i];
    printf("sum is:%f",s);
    printf("multiply:%f",p);

    }
    m=s/10;
    printf("average:%f\n",m);


    }
    return 0;
}
